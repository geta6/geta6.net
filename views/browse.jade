for item in items
  a.data.body(href=item.path.replace('/media/var', ''))
    .each.name(data-sort=item.name)= item.name
    .each.star(data-sort=item.star.length)= item.star.length
    .each.note(data-sort=item.note.length)= item.note.length
    .each.date(data-sort=item.date)= _.date(item.date).fromNow()
.data.foot
  .each total #{items.length}

